{
  "name": "hobbidu",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build",
    "format": "prettier --write ./app",
    "lint": "eslint ./app --max-warnings=0",
    "lint-fix": "eslint ./app --fix",
    "prepare": "husky install",
    "seed": "npx prisma db seed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(json|yml|yaml|css|less|scss|html)": [
      "npm run format"
    ],
    "*.+(js|jsx|ts|tsx)": [
      "npm run lint-fix",
      "npm run format"
    ]
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.10",
    "@prisma/client": "^4.10.1",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/server-runtime": "1.13.0",
    "@remix-run/vercel": "^1.13.0",
    "@vercel/node": "^2.9.6",
    "bcryptjs": "^2.4.3",
    "framer-motion": "^9.0.7",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-places-autocomplete": "^3.4.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-auth-socials": "^2.0.4",
    "remix-params-helper": "^0.4.10",
    "@remix-validated-form/with-zod": "^2.0.5",
    "remix-validated-form": "^4.6.8",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.13.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "esbuild": "^0.17.10",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
